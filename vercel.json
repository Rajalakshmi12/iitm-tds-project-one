{
    "version": 2,
    "functions": {
      "api/project_api.py": {
        "runtime": "python"
      }
    },
    {
    "builds": [
      {
        "src": "api/project_api.py",
        "use": "@vercel/python"
      }
    ]
  },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/api/project_api.py"
      }
    ]
  }